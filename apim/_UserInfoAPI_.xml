<?xml version="1.0" encoding="UTF-8"?>
<api context="/userinfo" name="_WSO2AMUserInfoAPI_" xmlns="http://ws.apache.org/ns/synapse">
  <resource faultSequence="_token_fault_" methods="GET" protocol="http" url-mapping="/*">
    <inSequence>
      <property expression="get-property('keyManager.port')"
        name="uri.var.portnum" scope="default" type="STRING"/>
      <property expression="get-property('keyManager.hostname')"
        name="uri.var.hostname" scope="default" type="STRING"/>
      <send/>
    </inSequence>
    <outSequence>
      <send/>
    </outSequence>
  </resource>
  <handlers>
    <handler class="org.wso2.carbon.apimgt.gateway.handlers.common.SynapsePropertiesHandler"/>
  </handlers>
</api>
